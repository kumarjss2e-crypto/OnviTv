{
  "expo": {
    "name": "OnviTV",
    "slug": "onvitv-reactnative",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#1e1b4b"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "14.0",
            "useFrameworks": "static"
          },
          "android": {
            "minSdkVersion": 21
          }
        }
      ],
      [
        "@react-native-firebase/app",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      [
        "react-native-video",
        {
          "enableNotificationControls": false
        }
      ]
    ],
    "notification": {
      "icon": "./assets/notification-icon.png",
      "color": "#7C3AED",
      "androidMode": "default",
      "androidCollapsedTitle": "OnviTV"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.onvitv.com",
      "buildNumber": "1.0.0",
      "deploymentTarget": "14.0",
      "googleServicesFile": "./ios/GoogleService-Info.plist",
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ],
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        },
        "ITSAppUsesNonExemptEncryption": false,
        "CFBundleURLTypes": [
          {
            "CFBundleURLName": "GoogleSignIn",
            "CFBundleURLSchemes": ["com.googleusercontent.apps.1035586796015-3e7fqts0ftqo7uppq4nchpu7vfl8pasp"]
          }
        ],
        "UIAppFonts": [
          "Ionicons.ttf"
        ]
      }
    },
    "android": {
      "package": "com.onvitv.com",
      "googleServicesFile": "./android/app/google-services.json",
      "useNextNotificationsApi": true,
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "WAKE_LOCK"
      ]
    },
    "web": {
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "9462daaf-0d04-412c-8851-23877824ffcf"
      }
    },
    "owner": "kumar2003"
  }
}
