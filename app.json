{
  "expo": {
    "name": "OnviTV",
    "slug": "onvitv-reactnative",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#1e1b4b"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "16.0",
            "cocoapods": {
              "deterministic_uuids": false
            }
          },
          "android": {
            "minSdkVersion": 21
          }
        }
      ]
    ],
    "notification": {
      "icon": "./assets/notification-icon.png",
      "color": "#7C3AED",
      "androidMode": "default",
      "androidCollapsedTitle": "OnviTV"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.onvitv.com",
      "buildNumber": "1.0.0",
      "deploymentTarget": "16.0",
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ],
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        },
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.onvitv.com",
      "useNextNotificationsApi": true,
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "WAKE_LOCK"
      ]
    },
    "web": {
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "9462daaf-0d04-412c-8851-23877824ffcf"
      }
    },
    "owner": "kumar2003"
  }
}
